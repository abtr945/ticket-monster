{
   "kind":"Service",
   "apiVersion":"v1beta3",
   "metadata":{
      "name":"appserver",
      "labels":{
         "name":"appserver"
      }
   },
   "spec":{
      "createExternalLoadBalancer": true,
      "ports": [
         {
           "name":"app-server-port",
           "port":8080,
           "targetPort":"app-server",
           "protocol":"TCP"
         },
         {
           "name":"admin-server-port",
           "port":9990,
           "targetPort":"admin-server",
           "protocol":"TCP"
         }
      ],
      "selector":{
         "name":"appserver"
      }
   }
}
